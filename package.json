{
  "name": "creative.network.server",
  "version": "0.0.1",
  "description": "Server side for the creative network",
  "main": "server.js",
  "scripts": {
    "start": "nodemon ./server.js --exec babel-node",
    "lint": "eslint"
  },
  "author": "Avi Zurel <avi@avi.io>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-loader": "^6.4.1",
    "babel-plugin-inline-import": "^2.0.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "graphql": "^0.9.3",
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "apollo-server": "^0.3.3",
    "express": "^4.15.2"
  },
  "peerDependencies": {
    "graphql": "^0.9.3"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb/base",
      "plugin:import/errors"
    ],
    "rules": {
      "no-use-before-define": 0,
      "arrow-body-style": 0,
      "dot-notation": 0,
      "no-console": 0
    },
    "env": {
      "mocha": true
    }
  }
}
